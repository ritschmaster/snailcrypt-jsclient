{
	"_version": "1.12.0",
	"sap.app": {
		"id": "snailcrypt-jsclient",
		"type": "application"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.75.0",
			"libs": {
				"sap.ui.core": {},
                "sap.ui.integration": {},
                "sap.ui.support": {},
                "sap.ui.table": {},
                "sap.ui.testrecorder": {},
				"sap.m": {}
			}
		},
		"rootView": {
			"viewName": "snailcrypt-jsclient.view.Main",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "snailcrypt-jsclient.i18n.i18n",
					"supportedLocales": ["en", "de"],
					"fallbackLocale": "en"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		}
	}
}
